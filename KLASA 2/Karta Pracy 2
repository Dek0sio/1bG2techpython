# karta pracy 2

# zad 1
def oblicz_czy_podzielna_przez_3():
    a = int(input())
    return a % 3 == 0
print(oblicz_czy_podzielna_przez_3())

#bajtoÅ¼abka
p = int(input("podaj punkt startu: "))
k = int(input("podaj punkt koncowy: "))
s = int(input("podaj dlugosc skokow: "))

if p + s * 3 > k:
    print("tak")
else:
    print("nie")
